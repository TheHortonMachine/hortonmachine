sim = new oms3.SimBuilder(logging:'ALL').sim(name:'pitfiller') {
   model {
      components  {
        'pitfiller'         'pit'
        'reader'            'rasterreader'
        'writer'            'rasterwriter'
      }

      parameter  {
        'reader.file'       '.\\testdataset\\elevation.asc'
        'writer.file'       '.\\testdataset\\pit.asc'
      }

      connect  {
        'reader.outRaster'  'pitfiller.inElev'
        'pitfiller.outPit'   'writer.inRaster'
      }

   }
}
sim.run();

